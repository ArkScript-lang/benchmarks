(let ackermann (fun (m n)
    (if (> m 0)
        (if (= 0 n)
            (ackermann (- m 1) 1)
            (ackermann (- m 1) (ackermann m (- n 1))))
        (+ 1 n))))

(mut i 0)
(let limit 1000)

(let start (time))

(while (< i limit) {
    (ackermann 3 6)
    (set i (+ i 1))
})
(let end (- (time) start))
(print "ArkScript " (* (/ end limit) 1000) " ms")
